<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="android:Theme.Material.Light.DarkActionBar">
        <!-- Customize your theme here. -->
    </style>
    <declare-styleable name="PQRegulatorView">
        <attr name="pqr_width" format="integer"/>
        <attr name="pqr_height" format="integer"/>
        <attr name="pq_type" format="enum">
            <enum name="R_gain" value="00"/>
            <enum name="G_gain" value="01"/>
            <enum name="B_gain" value="02"/>

            <enum name="R_offset" value="10"/>
            <enum name="G_offset" value="11"/>
            <enum name="B_offset" value="12"/>
        </attr>
    </declare-styleable>
</resources>
